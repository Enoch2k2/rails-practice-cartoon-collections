<%= render "layouts/wrapper" do %>
  <h3 class="center teal-text text-darken-4">Create Cartoon</h3>
  <div class="container" style="margin-bottom: 20px">
    <%= render "cartoons/form_errors" %>
    <%= form_for @cartoon do |f| %>
      <div class="input-field">
        <%= f.text_field :name %>
        <%= f.label :cartoon_name %>
      </div>

      <%= f.submit "Create Cartoon", class: "btn teal darken-4" %>
    <% end %>
  </div>
<% end %>